{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "prefer-const": "error",
    "no-var": "error"
  },
  "globals": {
    "ryttereData": "writable",
    "sprinterKort": "readonly",
    "allaroundKort": "readonly",
    "aktivSpiller": "writable",
    "spilletilstand": "writable",
    "opretRyttere": "readonly",
    "nytSpil": "readonly",
    "blandAlle": "readonly",
    "trækKort": "readonly",
    "vælgKort": "readonly",
    "tilføjTræthed": "readonly",
    "visNotifikation": "readonly",
    "visMmenu": "readonly",
    "skjulMenu": "readonly",
    "nytRunde": "readonly",
    "skjulResultater": "readonly",
    "tilføjTræthedResultat": "readonly"
  }
}
